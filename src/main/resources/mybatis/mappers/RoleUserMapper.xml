<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.vworld.mapper.RoleUserMapper">


    <insert id="saveNormalRole">
        INSERT into role_user VALUES (#{userId},1)
    </insert>

    <delete id="deleteRoleUserById">
        delete from role_user where role_id =#{roleId}
    </delete>

</mapper>
